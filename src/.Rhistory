summarize(avg_1stIn = mean(X1stIn, na.rm = TRUE),
n_games = n()) %>% #pipe it right into the ggplot2 - no temporary data frames floating around
ggplot() +
geom_line(aes(x = year, y = avg_1stIn)) +
geom_point(aes(x = year, y = avg_1stIn, size = n_games)) +
labs(title = "Average First-Serve In by Round",
y = "Avg 1st Serve In",
x = "Year") +
scale_size("Games\nPlayed")
cdata %>%
(year) %>%
summarize(count = n())
